   "source": [
    "class Solution:\n",
    "\n",
    "    def mySqrt(self, x: int) -> int:\n",
    "        l, r = 0, x\n",
    "        while l <= r:\n",
    "            m = l + r >> 1\n",
    "            if m * m <= x:\n",
    "                l = m + 1\n",
    "            else:\n",
    "                r = m - 1\n",
    "        return r\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "s.mySqrt(8)"
   ]
