    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "\n",
    "    def maxProfit(self, prices: List[int]) -> int:\n",
    "        minPrice = float('-inf')\n",
    "        ans = 0\n",
    "        for n in prices:\n",
    "            ans = max(ans, n - minPrice)\n",
    "            minPrice = min(minPrice, n)\n",
    "        return ans"
   ]
  }
