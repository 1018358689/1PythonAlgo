   "source": [
    "class Solution:\n",
    "\n",
    "    def fib(self, n: int) -> int:\n",
    "        if n == 0:\n",
    "            return 0\n",
    "        dp = [0] * (n + 1)\n",
    "        dp[0], dp[1] = 0, 1\n",
    "        for i in range(2, n + 1):\n",
    "            dp[i] = dp[i - 1] + dp[i - 2]\n",
    "        return dp[-1] % (10**9 + 7)\n"
   ]
  }
