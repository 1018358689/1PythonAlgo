    "def f(arr: list) -> None:\n",
    "    for i in range(1, len(arr)):\n",
    "        key = arr[i] # 要插入的值\n",
    "        j = i - 1 # 插入值前需要插入的范围[0,j]\n",
    "        while j >= 0 and key < arr[j]:\n",
    "            arr[j + 1] = arr[j]\n",
    "            j -= 1\n",
    "        arr[j + 1] = key\n"
   ]
  }
