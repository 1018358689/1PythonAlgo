   "source": [
    "def f(nums: list):\n",
    "    l, r = 0, len(nums) - 1\n",
    "    while l <= r:\n",
    "        m = (l + r) // 2\n",
    "        middle = nums[m]\n",
    "        if middle >= nums[0]:\n",
    "            l = m + 1\n",
    "        else:\n",
    "            r = m - 1\n",
    "    if l > len(nums) - 1:\n",
    "        return nums[0]\n",
    "    else:\n",
    "        return nums[l]\n"
   ]
