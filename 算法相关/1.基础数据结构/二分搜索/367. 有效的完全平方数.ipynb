    "# [1,2,3,4,5,6...] ->16\n",
    "def f(num: int):\n",
    "    l, r = 1, num  # 1, 46341\n",
    "    while l <= r:\n",
    "        m = (l + r) // 2\n",
    "        if m**2 >= num:\n",
    "            r = m - 1\n",
    "        else:\n",
    "            l = m + 1\n",
    "    return l**2 == num\n",
    "\n",
    "\n",
    "f(16), f(14)\n"
