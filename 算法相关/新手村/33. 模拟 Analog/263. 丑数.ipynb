   "source": [
    "class Solution:\n",
    "\n",
    "    def isUgly(self, n: int) -> bool:\n",
    "        if n <= 0:\n",
    "            return False\n",
    "        for i in (2, 3, 5):\n",
    "            while not n % i:\n",
    "                n //= i\n",
    "        return n == 1\n"
   ]
  }
