   "source": [
    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "\n",
    "    def constructArray(self, n: int, k: int) -> List[int]:\n",
    "        nums = list(range(1, n + 1))\n",
    "        for i in range(1, k):\n",
    "            nums[i:] = nums[i:][::-1]\n",
    "        return nums"
   ]
