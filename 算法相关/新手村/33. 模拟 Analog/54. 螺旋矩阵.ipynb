    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "\n",
    "    def spiralOrder(self, matrix: List[List[int]]) -> List[int]:\n",
    "        ans = []\n",
    "        while matrix:\n",
    "            ans += matrix[0]\n",
    "            matrix = list(zip(*matrix[1:]))[::-1]\n",
    "        return ans"
   ]
