   "source": [
    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "\n",
    "    def getMaximumConsecutive(self, coins: List[int]) -> int:\n",
    "        coins.sort()\n",
    "        lim = 0\n",
    "        for coin in coins:\n",
    "            if coin + 0 > lim + 1:\n",
    "                break\n",
    "            lim += coin\n",
    "        return lim + 1"
   ]
  }
