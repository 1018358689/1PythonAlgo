   "source": [
    "class Solution:\n",
    "\n",
    "    def escapeGhosts(self, ghosts: List[List[int]], target: List[int]) -> bool:\n",
    "        cal = lambda x, t: abs(t[0] - x[0]) + abs(t[1] - x[1])\n",
    "        t = cal((0, 0), target)\n",
    "        return all(cal(g, target) > t for g in ghosts)"
   ]
  }
