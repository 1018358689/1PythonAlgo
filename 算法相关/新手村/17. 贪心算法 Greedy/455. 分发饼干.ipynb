   "source": [
    "def f(g: list, s: list):\n",
    "    g.sort()\n",
    "    s.sort()\n",
    "    p1 = p2 = 0\n",
    "    while p1 < len(g) and p2 < len(s):\n",
    "        if s[p2] >= g[p1]: # 当满足饼干能喂饱该孩子则 下一个孩子\n",
    "            p1 += 1\n",
    "        p2 += 1 # 下一个饼干\n",
    "    return p1"
   ]
  }
