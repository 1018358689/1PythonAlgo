   "source": [
    "def f(nums: list):\n",
    "    maxLength = 0\n",
    "    for i, n in enumerate(nums):\n",
    "        if maxLength < i:\n",
    "            return False\n",
    "        maxLength = max(maxLength, i + n)\n",
    "        if maxLength >= len(nums) - 1:\n",
    "            return True\n",
    "\n",
    "\n",
    "f([2, 3, 1, 1, 4]), f([3, 2, 1, 0, 4])\n"
   ]
