    "def f(m, n):\n",
    "    # dp[i][j] 达到坐标[i,j]\n",
    "    # dp[i][0] dp[0][j] = 1\n",
    "    # dp[i][j] = dp[i-1][j]+dp[i][j-1]\n",
    "    dp = [[1] * n for _ in range(m)]\n",
    "    for i in range(1, m):\n",
    "        for j in range(1, n):\n",
    "            dp[i][j] = dp[i - 1][j] + dp[i][j - 1]\n",
    "    return dp[-1][-1]\n",
    "\n",
    "\n",
    "f(3, 7)\n"
   ]
