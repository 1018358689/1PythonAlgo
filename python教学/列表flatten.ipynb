   "source": [
    "def flatten(li):\n",
    "    res = []\n",
    "    for l in li:\n",
    "        if isinstance(l, list):\n",
    "            res += flatten(l)\n",
    "        else:\n",
    "            res += [l]\n",
    "    return res\n",
    "\n",
    "\n",
    "flatten(l)"
   ],
